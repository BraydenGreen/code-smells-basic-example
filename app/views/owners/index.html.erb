<h1>Listing Owners</h1>
<br>

<%= link_to 'New Owner', new_owner_path %>

<br>
<br>


  
    <% @owners.each do |owner| %>
      
       <h3><%= owner.first_name %> <%= owner.last_name %></h3>        
       <%= link_to 'Show', owner_path(owner) %>
       <%= link_to 'Edit', edit_owner_path(owner) %>
       <%= link_to 'Destroy', owner_path(owner), method: :delete, data: { confirm: 'Are you sure?' } %>
      
    <% end %>
  



